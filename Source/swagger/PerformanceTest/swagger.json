{
  "swagger": "2.0",
  "info": {
    "title": "Performance Test",
    "version": "1.0.0"
  },
  "host": "testserver",
  "schemes": [
    "http",
    "https"
  ],
  "basePath": "/v1",
  "produces": [
    "application/json"
  ],
  "paths": {
    "/test": {
      "post": {
        "tags": [
          "test"
        ],
